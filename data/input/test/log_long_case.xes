<?xml version="1.0" encoding="UTF-8" ?>
<log xes.version="1.0" xes.features="nested-attributes"
     xmlns="http://www.xes-standard.org/">

  <extension name="Concept" prefix="concept" uri="http://www.xes-standard.org/concept.xesext"/>
  <extension name="Time" prefix="time" uri="http://www.xes-standard.org/time.xesext"/>

  <global scope="trace">
    <string key="concept:name" value="case"/>
  </global>

  <global scope="event">
    <string key="concept:name" value="activity"/>
    <date key="time:timestamp" value="1970-01-01T00:00:00.000+00:00"/>
  </global>

  <trace>
    <string key="concept:name" value="case_long_001"/>

    <!-- Inicio del caso -->
    <event>
      <string key="concept:name" value="register_user"/>
      <date key="time:timestamp" value="2024-01-01T09:00:00.000+00:00"/>
    </event>

    <!-- Primer acceso a datos personales -->
    <event>
      <string key="concept:name" value="access_user_profile"/>
      <date key="time:timestamp" value="2024-01-05T10:00:00.000+00:00"/>
    </event>

    <!-- Modificación de datos -->
    <event>
      <string key="concept:name" value="update_user_profile"/>
      <date key="time:timestamp" value="2024-01-10T11:30:00.000+00:00"/>
    </event>

    <!-- Nuevo acceso -->
    <event>
      <string key="concept:name" value="access_user_profile"/>
      <date key="time:timestamp" value="2024-02-01T09:00:00.000+00:00"/>
    </event>

    <!-- Acceso muy posterior (permite expiración / borrado) -->
    <event>
      <string key="concept:name" value="access_user_profile"/>
      <date key="time:timestamp" value="2025-02-15T10:00:00.000+00:00"/>
    </event>

    <!-- Cierre -->
    <event>
      <string key="concept:name" value="close_account"/>
      <date key="time:timestamp" value="2025-03-01T12:00:00.000+00:00"/>
    </event>

  </trace>
</log>
